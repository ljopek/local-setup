[user]
	email = lukasz.jopek@gmail.com
	name = Lukasz Jopek
[alias]
	ec = config --global -e
	save = !git add -A && git commit -m "SAVEPOINT"
	undo = reset HEAD~1 --mixed
	wipe = !git add -A && git commit -qm "WIPE SAVEPOINT" && git reset HEAD~1 --hard
	amend = commit -a --amend
	bck = !git branch bck_`date '+%Y-%m-%d@%Hh%Mm%Ss'` && git branch --list
	st = status
	ignore-date = rebase origin/master --ignore-date
	rb2 = rebase -i HEAD~2
	rb3 = rebase -i HEAD~3
	rb4 = rebase -i HEAD~4
	rb5 = rebase -i HEAD~5
	rb6 = rebase -i HEAD~6
	rb7 = rebase -i HEAD~7
	rb8 = rebase -i HEAD~8
	rb9 = rebase -i HEAD~9
	rb10 = rebase -i HEAD~10
	log1 = log -1
[help]
	autocorrect = 1
